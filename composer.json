{
    "name": "cmercado93/laravel-simple-sitemap",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "Cmercado93\\LaravelSimpleSitemap\\": "src/"
        },
        "files": [
            "src/Common/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Cmercado93\\LaravelSimpleSitemap\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Carlos Mercado",
            "email": "cmercado93@outlook.com"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1",
        "illuminate/http": "^5.8",
        "illuminate/routing": "^5.8",
        "illuminate/support": "^5.8"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Cmercado93\\LaravelSimpleSitemap\\SiteMapProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^7.5",
        "orchestra/testbench": "^3.8"
    }
}
